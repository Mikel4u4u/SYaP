;gnu clisp 2.49

(defun raznost ( lst )
 

  ( cond  
      
   ((null (cdr lst) ) T )    
   ((oddp   ( - (cadr lst)  (car lst )))   ( raznost ( cdr lst ))) 

   )
     
    
 )
   

( print (raznost '(1 4 9 16 25 )))


(defun qwadratnat ( num )
 
    (cond
      ((eq num 0) nil )
     (t (      
         cons  (- num 1 ) num
         
         ))
    
 )

( print (qwadratnat '(4 )))
