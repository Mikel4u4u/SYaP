8. Реализовать на языке Haskell функцию, которая увеличивает элементы исходного списка на единицу

f :: [Integer] -> [Integer]
f [ ] = [ ]
f [a] = [a+1]
f (a:t) = a+1 :f t
main = print $ f [1 , 2 , 3 , 4 , 5 ]
;[2,3,4,5,6]



incr a = a + 1
f :: [Integer] -> [Integer]
f (t) = map incr  t
main = print $ f [1 , 2 , 3 , 4 , 5 ]
;[2,3,4,5,6]
