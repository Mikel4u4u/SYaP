--9. Реализовать на языке Haskell функцию, которая увеличивает элементы исходного списка в 10 раз.

f :: [Integer] -> [Integer]
f [ ] = [ ]
f [a] = [a*10]
f (a:t) = a*10 :f t
main = print $ f [1 , 2 , 3 , 4 , 5 ]

--[10,20,30,40,50]

f :: [Integer] -> [Integer]
f (t) = map  (* 10 )  t
main = print $ f [1 , 2 , 3 , 4 , 5 ]

--[10,20,30,40,50]
